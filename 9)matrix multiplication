#include<stdio.h>
int main(){
    int i,j,k,m1[2][2],m2[2][2],sum[2][2];
    printf("enter the elements of m1:\n");
    for(i=0;i<2;i++){
        for(j=0;j<2;j++){
            scanf("%d",&m1[i][j]);
        }
    }
    printf("enter the elements of m2:\n");
    for(i=0;i<2;i++){
        for(j=0;j<2;j++){
            scanf("%d",&m2[i][j]);
        }
    }
    for(i=0;i<2;i++){
        for(j=0;j<2;j++){
            sum[i][j]=0;
        }
    }
    for(i=0;i<2;i++){
        for(j=0;j<2;j++){
            for(k=0;k<2;k++){
                sum[i][j]+=m1[i][k]*m2[k][j];
            }
        }
    }
    printf("product of 2 matrices:\n");
    for(i=0;i<2;i++){
        for(j=0;j<2;j++){
            printf(" %d ",sum[i][j]);
        }
        printf("\n");
    }
    return 0;
}
