#include<stdio.h>
int main(){
    int i,j,a[]={2,8,9,4,1},size,temp;
    size=sizeof(a)/sizeof(a[0]);
    for(i=0;i<size;i++){
        for(j=i+1;j<size;j++){
            if(a[i]>a[j]){
                temp=a[i];
                a[i]=a[j];
                a[j]=temp;
            }
        }
    }
    printf("bubble sort");
    for(i=0;i<size;i++){
        printf(" %d ",a[i]);
    }
    return 0;
}
