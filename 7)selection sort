#include<stdio.h>
int main(){
    int i,j,size,temp,minindex;
    int a[]={3,2,8,5,1,9};
    size=sizeof(a)/sizeof(a[0]);
    for(i=0;i<size;i++){
        minindex=i;
        for(j=i+1;j<size;j++){
            if(a[j]<a[minindex]){
                minindex=j;
            }
        }
        if(minindex!=i){
            temp=a[i];
            a[i]=a[minindex];
            a[minindex]=temp;
        }
    }
    for(i=0;i<size;i++){
        printf(" %d ",a[i]);
    }
    return 0;
}
