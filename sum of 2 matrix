#include<stdio.h>
int main(){
    int m1[2][2],m2[2][2],i,j,sum[2][2];
    printf("enter the elements og m1:\n");
    for(i=0;i<2;i++){
        for(j=0;j<2;j++){
            scanf("%d",&m1[i][j]);
        }
    }
    printf("enter the elements of m2:\n");
    for(i=0;i<2;i++){
        for(j=0;j<2;j++){
            scanf("%d",&m2[i][j]);
        }
    }
    printf("sum of the matrices:\n");
    for(i=0;i<2;i++){
        for(j=0;j<2;j++){
            sum[i][j]=m1[i][j]+m2[i][j];
            printf(" %d ",sum[i][j]);
        }
        printf("\n");
    }
    return 0;
}
