#include<stdio.h>
int main(){
    int i,n,num,sum=0,r;
    printf("enter the number:");
    scanf("%d",&n);
    num=n;
    while(n>0){
        r=n%10;
        sum+=r*r*r;
        n/=10;
    }
    if(sum==num){
        printf("armstrong number");
    }else{
        printf("not armstrong number");
    }
}
